                <annotation/>
            </element>
            <element name="pipe.transport" maxOccurs="1" minOccurs="0" type="anyURI">
                <annotation/>
            </element>
            <element name="tcp.transport" maxOccurs="1" minOccurs="0" type="anyURI">
