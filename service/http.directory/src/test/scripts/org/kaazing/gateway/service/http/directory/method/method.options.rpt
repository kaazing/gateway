connect tcp://localhost:8000
connected

write "OPTIONS / HTTP/1.1\r\n"
write "Host: localhost:8000\r\n"
write "\r\n"
write notify BARRIER

write await BARRIER
read "HTTP/1.1 405 Method Not Allowed\r\n"
read "Content-Type: text/html\r\n"
read /Date:.*/ "\r\n"
read "Server: Kaazing Gateway\r\n"
read /Content-Length: .*/ "\r\n"
read "\r\n"
read "<html><head></head><body><h1>405 Method Not Allowed</h1></body></html>"

close
closed


