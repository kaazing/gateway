# curl 7.37.1 <---> Gateway <---> tomcat 8.0.1

connect tcp://localhost:8110
connected

write "HEAD /manager/ HTTP/1.1\r\n"
write "User-Agent: curl/7.37.1\r\n"
write "Host: localhost:8110\r\n"
write "Accept: */*\r\n"
write "\r\n"

read "HTTP/1.1 403 Forbidden\r\n"
read "Content-Type: text/html;charset=ISO-8859-1\r\n"
read /Date: .*/ "\r\n"
read "Server: Apache-Coyote/1.1\r\n"
read "Set-Cookie: JSESSIONID=DEB431EAE851837DEF87291EF28E62B7; Path=/manager/; HttpOnly\r\n"
read "Transfer-Encoding: chunked\r\n"
read "\r\n"

close
closed

#
# tomcat server
#

accept tcp://localhost:8080
accepted
connected

read "HEAD /manager/ HTTP/1.1\r\n"
read "X-Forwarded-For: 127.0.0.1\r\n"
read "Via: 1.1 kaazing\r\n"
read "User-Agent: curl/7.37.1\r\n"
read "Host: localhost:8110\r\n"
read "Connection: close\r\n"
read "Accept: */*\r\n"
read "\r\n"

write "HTTP/1.1 403 Forbidden\r\n"
write "Server: Apache-Coyote/1.1\r\n"
write "Set-Cookie: JSESSIONID=DEB431EAE851837DEF87291EF28E62B7; Path=/manager/; HttpOnly\r\n"
write "Content-Type: text/html;charset=ISO-8859-1\r\n"
write "Transfer-Encoding: chunked\r\n"
write "Date: Tue, 17 Feb 2015 23:04:03 GMT\r\n"
write "\r\n"

close
closed
