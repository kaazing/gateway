connect http://localhost:8110/acceptConfigPath/example/jsp/num/file.html
connected

write method "GET"
write parameter "name" "value"
write version "HTTP/1.1"

write header "User-Agent" "curl/7.37.1"
write header "Host" "localhost:8110"
write header "Accept" "*/*"
write close

read status "200" "OK"
read version "HTTP/1.1"
read header "Content-Length" "17"
read header "Content-Type" "text/html;charset=ISO-8859-1"
#read closed

close
closed


accept http://localhost:8080/connectConfigPath/example/jsp/num/file.html?name=value
accepted
connected

read method "GET"
read header "Via" "1.1 kaazing"
read header "User-Agent" "curl/7.37.1"
read header "Host" "localhost:8080"
read header "Forwarded" "for=127.0.0.1"
read header "Connection" "close"
read header "Accept" "*/*"
read closed

write status "200" "OK"
write version "HTTP/1.1"

write header "Content-Type" "text/html;charset=ISO-8859-1"
write header "Connection" "close"
write header content-length
write close

closed
