# curl 7.37.1 <---> Gateway <---> tomcat 8.0.1

connect tcp://localhost:8110
connected

write "GET /info HTTP/1.1\r\n"
write "Host: localhost:8111\r\n"
write "Accept: */*\r\n"
write "\r\n"

read "HTTP/1.1 401 Unauthorized\r\n"

close
closed

#
# tomcat server
# Request is not forwarded to upstream
#
accept tcp://localhost:8081

