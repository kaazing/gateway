# curl 7.37.1 <---> Gateway <---> tomcat 8.0.1

connect tcp://localhost:8110
connected

write "GET / HTTP/1.1\r\n"
write "User-Agent: curl/7.37.1\r\n"
write "Host: localhost:8110\r\n"
write "Accept: */*\r\n"
write "\r\n"

read "HTTP/1.1 504 Gateway Timeout\r\n"
read "Content-Type: text/html\r\n"
read /Date: .*/ "\r\n"
read "Server: Kaazing Gateway\r\n"
read /Content-Length: .*/ "\r\n"
#read /[^504]*/ "504 Gateway Timeout" /.*/
read "\r\n<html><head></head><body><h1>504 Gateway Timeout</h1></body></html>"
read /.*/

closed

#
# tomcat server
#

accept tcp://localhost:8080
accepted
connected

read "GET / HTTP/1.1\r\n"
read "Via: 1.1 kaazing\r\n"
read "User-Agent: curl/7.37.1\r\n"
read "Host: localhost:8110\r\n"
read "Forwarded: for=127.0.0.1\r\n"
read "Connection: close\r\n"
read "Accept: */*\r\n"
read "\r\n"

#write "HTTP/1.1 200 OK\r\n"
#write "\r\n"

close
closed
