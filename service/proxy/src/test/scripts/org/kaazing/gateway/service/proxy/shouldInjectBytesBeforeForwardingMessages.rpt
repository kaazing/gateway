connect tcp://localhost:8001
connected

write "hi"

read "hi"

# connection will be closed by the "echo server" stream below
closed

############################################################ Backend Echo Service stream ###################################################################

accept tcp://localhost:8002
accepted
connected

read "injected"
read "hi"

# echo "hi"
write "hi"

# This time close the connection
close
closed
