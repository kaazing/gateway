accept http://localhost:8080/jms
accepted
connected

read method "GET"
read version "HTTP/1.1"
read header "Upgrade" "websocket"
read header "Sec-WebSocket-Version" "13"
read header "Sec-WebSocket-Key" "dGhlIHNhbXBsZSBub25jZQ=="
#read header "Origin" "http://localhost:8080"
read header "Host" "localhost:8080"
read header "Connection" "Upgrade"
read closed


write status "302" "Found"
write header "Location" "http://localhost:8081/jms"
write header content-length
write close


accept http://localhost:8081/jms
accepted
connected

read method "GET"
read version "HTTP/1.1"
read header "Upgrade" "websocket"
read header "Sec-WebSocket-Version" "13"
read closed

