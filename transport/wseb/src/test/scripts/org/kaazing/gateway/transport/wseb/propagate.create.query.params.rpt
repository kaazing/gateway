connect http://localhost:8000/echo/;e/ct
connected

write method "GET"
write parameter "param" "value"
write header host
write header "X-Origin" "http://localhost:8000"
write header "X-WebSocket-Version" "wseb-1.0"
write header content-length
write close

read status "200" "OK"
read header "Cache-Control" "no-cache"
read header "Content-Type" "text/plain;charset=UTF-8"

read "HTTP/1.1 201 Created\r\n"
read "Content-Type: text/plain;charset=UTF-8\r\n"
read "\r\n"

read "http://localhost:8000/echo/;e/ut/" /[0-9a-zA-Z]{32}/ "?param=value\n"
read "http://localhost:8000/echo/;e/dt/" /[0-9a-zA-Z]{32}/ "?param=value\n"
read closed

